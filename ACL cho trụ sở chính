	ip access-group ACL1 in
	exit
int vlan 6
	ip access-group ACL1 in
	exit
int vlan 7
	ip access-group ACL1 in
	exit
int vlan 8
	ip access-group ACL1 in
	exit
int vlan 9
	ip access-group ACL1 in
	exit
int vlan 10
	ip access-group ACL1 in
	exit
	end
	wr
